#################################################################
#
#    Object Properties
#
#################################################################



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#causedByFault

:causedByFault rdf:type owl:ObjectProperty ;
                     
                     rdfs:range :Fault .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#hasComponent

:hasComponent rdf:type owl:ObjectProperty ;
                    
                    rdfs:domain :Yacht ;
                    
                    rdfs:range :YachtComponent .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#hasPoint

:hasPoint rdf:type owl:ObjectProperty ;
                
                rdfs:domain :Route ;
                
                rdfs:range :RoutePoint .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#hasPotentialSupplier

:hasPotentialSupplier rdf:type owl:ObjectProperty ;
                            
                            rdfs:range :PotentialSupplierOrganization .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#hasServiceType

:hasServiceType rdf:type owl:ObjectProperty ;
                      
                      rdfs:range :HarbourServiceType ;
                      
                      rdfs:domain :SmartYachtingService .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#hasSupportedService

:hasSupportedService rdf:type owl:ObjectProperty ;
                           
                           rdfs:range core:Service .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#supportsServiceType

:supportsServiceType rdf:type owl:ObjectProperty ;
                           
                           rdfs:range :HarbourServiceType  .




#################################################################
#
#    Data properties
#
#################################################################


###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#occurance

:occurance rdf:type owl:DatatypeProperty ;
                 
                 rdfs:range xsd:dateTime .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#timestamp

:timestamp rdf:type owl:DatatypeProperty ;
                 
                 rdfs:range xsd:dateTime .


###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#hint

:hint rdf:type owl:DatatypeProperty ;
            
            rdfs:range xsd:string .
			


#################################################################
#
#    Classes
#
#################################################################


###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#BlackWaterTank

:BlackWaterTank rdf:type owl:Class ;
                      
                      rdfs:subClassOf :Tank .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#ConsumableNeed

:ConsumableNeed rdf:type owl:Class ;
                      
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasPotentialSupplier ;
                                        owl:onClass :PotentialSupplierOrganization ;
                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty core:description ;
                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onDataRange rdf:langString
                                      ] .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#Exhaust

:Exhaust rdf:type owl:Class ;
               
               rdfs:subClassOf :YachtComponent .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#Fault

:Fault rdf:type owl:Class .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#FreshWaterTank

:FreshWaterTank rdf:type owl:Class ;
                      
                      rdfs:subClassOf :Tank .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#GreyWaterTank

:GreyWaterTank rdf:type owl:Class ;
                     
                     rdfs:subClassOf :Tank .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#HarbourServiceType

:HarbourServiceType rdf:type owl:Class .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#MaintenanceNeed

:MaintenanceNeed rdf:type owl:Class ;
                       
                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                         owl:onProperty core:description ;
                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange rdf:langString
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasPotentialSupplier ;
                                         owl:onClass :PotentialSupplierOrganization ;
                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :causedByFault ;
                                         owl:onClass :Fault ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :occurance ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:dateTime
                                       ] .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#Port

:Port rdf:type owl:Class ;
            
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :supportsServiceType ;
                              owl:onClass :HarbourServiceType ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                            ] .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#PortExhaust

:PortExhaust rdf:type owl:Class ;
                   
                   rdfs:subClassOf :Exhaust .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#PotentialSupplierOrganization

:PotentialSupplierOrganization rdf:type owl:Class .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#Route

:Route rdf:type owl:Class ;
             
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasPoint ;
                               owl:onClass :RoutePoint ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                             ] .


							 
###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#Waypoint

:Waypoint rdf:type owl:Class ;
                
                rdfs:subClassOf core:WGS84Location ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hint ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:string
                                ] .


###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#RoutePoint

:RoutePoint rdf:type owl:Class ;
                  
                  rdfs:subClassOf :Waypoint ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :timestamp ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onDataRange xsd:dateTime
                                  ] .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#ServiceFuelOilTank

:ServiceFuelOilTank rdf:type owl:Class ;
                          
                          rdfs:subClassOf :Tank .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#SmartYachtingService

:SmartYachtingService rdf:type owl:Class ;
                            
                            rdfs:subClassOf core:Service ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :hasServiceType ;
                                              owl:onClass :HarbourServiceType ;
                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                            ] .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#StarboardExhaust

:StarboardExhaust rdf:type owl:Class ;
                        
                        rdfs:subClassOf :Exhaust .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#StorageFuelOilTank

:StorageFuelOilTank rdf:type owl:Class ;
                          
                          rdfs:subClassOf :Tank .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#Tank

:Tank rdf:type owl:Class ;
            
            rdfs:subClassOf :YachtComponent .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#Yacht

:Yacht rdf:type owl:Class ;
             
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasComponent ;
                               owl:onClass :YachtComponent ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :supportsServiceType ;
                               owl:onClass :HarbourServiceType ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasSupportedService ;
                               owl:onClass :SmartYachtingService ;
                               owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger
                             ] .



###  http://www.symbiote-h2020.eu/ontology/bim/smartyachting#YachtComponent

:YachtComponent rdf:type owl:Class ; 
			
             rdfs:subClassOf core:FeatureOfInterest.



